//--- INFO BEGIN ---
//Mission information for the level select. Generated from the MissionInfo object except with extra goodies.
function PQ_Arkanoid_GetMissionInfo() {
	return
new ScriptObject() {
		name = "Arkanoid";
		type = "Bonus";
		level = "11";
		desc = "Marblaxia contacted DOH for help, and it had trapped you in this Breakout-type level. Clear the board to escape!";
		artist = "Matan";
		music = "Chilled.ogg";
		game = "PlatinumQuest";
		gameMode = "2D";
		platinumTime = "140000";
		ultimateTime = "105000";
		awesomeTime = "69000";
		generalHint = "Move right and left quickly while bouncing on walls to gain much-needed height.";
		ultimateHint = "Clear the right side\'s Time Travels and then go to the Dragon\'s mouth. Clear the top area and finish with the neck.";
		awesomeHint = "Gravity might be your very undoing here due to the speed your marble will move.\nNonetheless, you can use it to gain slight bounces every time you hit a brick, then hit a brick adjacent to it.\nThis way you can clear several bricks much more quickly while slowly falling down.";
		CustomRadarRule = $Radar::Flags::None;
		Arkanoid = "1";
		cameraPitch = "0.1";
		cameraPlane = "yz";
		easterEgg = "0";
		gems = "0";
		gravity = "3";
		initialCameraDistance = "27.5";
		interior0 = $usermods @ "/data/interiors_pq/bonus/ArkanoidDragon.dif";
		interiors = "1";
		maxScore = "0";
		requirements = "Complete the Economically Viable achievement.";
		trivia = "Originally meant to be part of the puzzle series along with Racing Spirits, the idea was left abandoned before picking it up at the end of development. It was created, tested and completed in just two hours.\nThis level was inspired by the famous Arkanoid games, which Matan spent too many hours on in his childhood.\n\nAlso, if you aren\'t humming to yourself the jingle that played at the start of every level, then what is wrong with you?!";
		unlockFunc = "unlock_Arkanoid";
};
}
//--- INFO END ---
//--- UNLOCK BEGIN ---
function unlock_Arkanoid(%mission) {
	return checkAchievementPref($Achievement::Category::Level, 5);
}
//--- UNLOCK END ---
