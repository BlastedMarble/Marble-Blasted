function addToArkanoidSet(%obj) {
	if (!isObject(ArkanoidHiddenFPSet)) {
		new SimSet(ArkanoidHiddenFPSet);
		MissionCleanup.add(ArkanoidHiddenFPSet);
	}
	ArkanoidHiddenFPSet.add(%obj);
	
	if (ArkanoidHiddenFPSet.getCount() == ArkanoidFPGroup.getCount()) {
		endGameSetup();
	}
}

function clientCbOnMissionReset() {
	ArkanoidHiddenFPSet.delete();
}

package Arkanoid {
	function StaticShape::hide(%this, %hidden) {
		Parent::hide(%this, %hidden);
		if (!MissionInfo.Arkanoid) {
			deactivatePackage(Arkanoid);
		}

		if (%hidden && %this.getGroup().getId() == ArkanoidFPGroup.getId()) {
			addToArkanoidSet(%this);
		}
	}
};

function MissionStartup() {
	activatePackage(Arkanoid);
}

