//In the event that you want this mission to be locked by a function, here's
// where you should put that. Just uncomment this function and fill it out:
function unlock_SacredGroundB1(%mission) {
	return (Unlock::getMissionCompletion(getMissionInfo("platinum/data/missions_pq/expert/Miscalculation.mcs")) & $Completion::Platinum) &&
		(Unlock::getGameCompletionCountFlags("PlatinumQuest", $Completion::EasterEgg) >= 15);
}
